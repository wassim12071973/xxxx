{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "api/index.py"
    },
    {
      "source": "/",
      "destination": "static/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "static/$1"
    }
  ]
}
